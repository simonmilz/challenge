{

  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": [false],
    "object-literal-sort-keys": [false],
    "jsx-no-lambda": false,
    "jsx-boolean-value": "never",
    "member-access": [false, "check-accessor"],
    "no-namespace": [false, "allow-declarations"]

  }
}
